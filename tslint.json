{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "interface-name": [true, "never-prefix"],
      "no-console": false,
      "quotemark": [true, "single"],
      "radix": false,
      "trailing-comma": [true, {
        "multiline": "never",
        "singleline": "never"
      }]
    },
    "rulesDirectory": []
}
